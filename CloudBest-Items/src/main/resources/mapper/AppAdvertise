<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cloudbest.items.entity.AppAdvertise">

    <resultMap type="com.cloudbest.items.entity.NewProducts" id="AppAdvertiseMap">
        <result property="id" column="id"/>
        <result property="img" column="img"/>
        <result property="description" column="description"/>
        <result property="status" column="status"/>
        <result property="groudingTime" column="grouding_time"/>
        <result property="validityTime" column="validity_time"/>
        <result property="type" column="type"/>
        <result property="tag" column="tag"/>
        <result property="position" column="position"/>
        <result property="createTime" column="create_time"/>
    </resultMap>

    <select id="selectAppAdvertises" resultMap="AppAdvertiseMap">
        select * from app_advertise where time between groudingTime and validity_time
    </select>

</mapper>



















